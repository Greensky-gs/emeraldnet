  "files": [
    "./dist",
    "index.d.ts"
  ],
  "directories": {
    "lib": "src"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./src/*": "./dist/*",
    "./dist/*": "./dist/*"
