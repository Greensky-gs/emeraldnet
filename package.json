  "license": "MIT",
  "dependencies": {
    "body-parser": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "request-ip": "^3.3.0",
    "tsc": "^2.0.4",
    "typescript": "^5.2.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/mysql": "^2.15.23",
